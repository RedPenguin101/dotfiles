{
   "languageserver": {
       "zls" : {
           "command": "~/zls/zls",
           "filetypes": ["zig"]
       },
       "clojure-lsp": {
           "command": "clojure-lsp",
           "filetypes": ["clojure"],
           "initializationOptions": {
             "ignore-classpath-directories": true
           }
       }
   }
}
